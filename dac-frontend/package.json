    "typescript": "~5.7.2",
    "vite": "^6.3.1"
  },
  "dependencies": {
    "@codemirror/lang-yaml": "^6.1.2",
    "@sveltestrap/sveltestrap": "^7.1.0",
    "axios": "^1.13.2",
    "sv-router": "^0.11.1",
    "svelte-codemirror-editor": "^2.1.0",
    "yaml": "^2.8.2"
  },
  "exports": {
